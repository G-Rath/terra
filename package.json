{
  "name": "terraport",
  "version": "0.0.0",
  "description": "Command-line tool for generating and importing assets into Terraform.",
  "homepage": "https://github.com/G-Rath/terraport#readme",
  "bugs": {
    "url": "https://github.com/G-Rath/terraport/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/G-Rath/terraport.git"
  },
  "license": "ISC",
  "author": "",
  "files": [
    "/oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "terraport": "./bin/terraport"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build-codebase && npm run build-manifest",
    "build-codebase": "npm run clean && ttsc -b tsconfig.build.json",
    "build-manifest": "node -r tsconfig-paths/register node_modules/.bin/oclif-dev manifest",
    "check": "tsc --noEmit",
    "clean": "rm -rf lib",
    "cli": "node -r tsconfig-paths/register bin/terraport",
    "lint": "eslint . --ignore-pattern '!.eslintrc.js' --ext js,ts",
    "prepack": "npm run build",
    "test": "jest --coverage"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "astTransformers": [
          "ts-nameof"
        ]
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    },
    "resetMocks": true,
    "restoreMocks": true,
    "setupFilesAfterEnv": [
      "./test/setupExpectEachTestHasAssertions.ts",
      "./test/setupAwsSdkMock.ts",
      "./test/matchers/index.ts"
    ],
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner",
    "transform": {
      "\\.tsx?": "ts-jest"
    }
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.3",
    "aws-sdk": "^2.595.0",
    "indent-string": "^4.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/jest": "^24.0.24",
    "@types/ts-nameof": "^4.2.0",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-nameof": "^4.2.2",
    "ts-node": "^8.5.4",
    "ts-transform-paths": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.8",
    "typescript": "^3.7.4"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "terraport",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  }
}
